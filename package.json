{
  "name": "jspsych-builder",
  "version": "4.1.1",
  "description": "A CLI utility to easily develop and package jsPsych experiments",
  "author": "bjoluc <mail@bjoluc.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bjoluc/jspsych-builder.git"
  },
  "bugs": {
    "url": "https://github.com/bjoluc/jspsych-builder/issues"
  },
  "homepage": "https://github.com/bjoluc/jspsych-builder#readme",
  "type": "module",
  "main": "src/index.js",
  "files": [
    "src",
    "assets"
  ],
  "bin": {
    "jspsych": "src/index.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "node ./tests/story.js",
    "commit": "git-cz"
  },
  "dependencies": {
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-transform-classes": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.17.0",
    "babel-loader": "^8.2.3",
    "chalk": "^5.0.0",
    "core-js": "^3.21.0",
    "css-loader": "^6.6.0",
    "deep-diff": "^1.0.2",
    "del": "^6.0.0",
    "delay": "^5.0.0",
    "execa": "^6.0.0",
    "glob": "^7.2.0",
    "glob-promise": "^4.2.2",
    "gulp": "^4.0.2",
    "gulp-cached": "^1.1.1",
    "gulp-file": "^0.4.0",
    "gulp-if": "^3.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-template": "^5.0.0",
    "gulp-zip": "^5.1.0",
    "html-webpack-plugin": "^5.5.0",
    "html-webpack-tags-plugin": "^3.0.2",
    "import-cwd": "^3.0.0",
    "inquirer": "^8.2.0",
    "jest-docblock": "^27.5.1",
    "listr": "^0.14.3",
    "listr-silent-renderer": "^1.1.1",
    "lodash-es": "^4.17.21",
    "mini-css-extract-plugin": "^2.5.3",
    "regenerator-runtime": "^0.13.9",
    "resolve-cwd": "^3.0.0",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "slash": "^4.0.0",
    "slugify": "^1.6.5",
    "update-notifier": "^5.1.0",
    "uuid": "^8.3.2",
    "webpack": "^5.68.0",
    "webpack-dev-server": "^4.7.4",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@bjoluc/semantic-release-config-npm": "^2.0.1",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@jspsych/plugin-fullscreen": "^1.1.0",
    "@jspsych/plugin-html-keyboard-response": "^1.1.0",
    "@jspsych/plugin-preload": "^1.1.0",
    "axios": "^0.25.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.4",
    "jspsych": "^7.1.2",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "shelljs": "^0.8.5"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 100
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "disableScopeLowerCase": true,
      "disableSubjectLowerCase": true
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "always",
        [
          "sentence-case"
        ]
      ],
      "scope-case": [
        0,
        "never",
        [
          "lower-case"
        ]
      ]
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.js": [
      "prettier --write"
    ]
  },
  "release": {
    "extends": "@bjoluc/semantic-release-config-npm"
  }
}

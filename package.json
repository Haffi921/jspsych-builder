{
  "name": "jspsych-builder",
  "version": "1.1.0",
  "description": "A CLI utility to easily develop and package jsPsych experiments",
  "author": "bjoluc <mail@bjoluc.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bjoluc/jspsych-builder.git"
  },
  "bugs": {
    "url": "https://github.com/bjoluc/jspsych-builder/issues"
  },
  "homepage": "https://github.com/bjoluc/jspsych-builder#readme",
  "main": "src/index.js",
  "files": [
    "src",
    "assets"
  ],
  "bin": {
    "jspsych": "src/index.js"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "test": "tests/story.js",
    "commit": "git-cz"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.0.0",
    "commander": "^5.0.0",
    "core-js": "^3.6.5",
    "css-loader": "3.5.2",
    "del": "^5.1.0",
    "delay": "4.3.0",
    "execa": "4.0.0",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "gulp": "^4.0.2",
    "gulp-file": "^0.4.0",
    "gulp-html-replace": "^1.6.2",
    "gulp-if": "^3.0.0",
    "gulp-inject": "^5.0.5",
    "gulp-load-plugins": "^2.0.3",
    "gulp-remove-empty-lines": "^0.1.0",
    "gulp-rename": "^2.0.0",
    "gulp-template": "5.0.0",
    "gulp-zip": "^5.0.1",
    "import-cwd": "^3.0.0",
    "inquirer": "^7.1.0",
    "jest-docblock": "^25.3.0",
    "listr": "^0.14.3",
    "mini-css-extract-plugin": "0.9.0",
    "regenerator-runtime": "^0.13.5",
    "resolve-cwd": "^3.0.0",
    "sass": "1.26.3",
    "sass-loader": "^8.0.2",
    "slugify": "1.4.0",
    "update-notifier": "4.1.0",
    "uuid": "^7.0.3",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.10.3"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.5",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "axios": "0.19.2",
    "commitizen": "4.0.4",
    "conventional-changelog-conventionalcommits": "^4.2.3",
    "cz-conventional-changelog": "3.1.0",
    "husky": "4.2.5",
    "lint-staged": "10.1.7",
    "prettier": "2.0.5",
    "semantic-release": "17.0.6",
    "shelljs": "0.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "always",
        [
          "sentence-case"
        ]
      ],
      "scope-case": [
        0,
        "never",
        [
          "lower-case"
        ]
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.js": [
      "prettier --write"
    ]
  }
}
